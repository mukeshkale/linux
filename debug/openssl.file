##To check the certificate details.
echo | openssl s_client -showcerts -servername nagios.fra1.appnexus.net -connect nagios.fra1.appnexus.net:443 2>/dev/null | openssl x509 -inform pem -noout -text

##OR

echo | openssl s_client -showcerts  -connect google.com:443 2>/dev/null | openssl x509 -inform pem -noout -text


